<ScrollPane xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controls.AddProjectControl">
   <content>
      <AnchorPane>
         <children>
            <VBox layoutX="-6.0">
               <children>
                  <Label fx:id="labelAddProject" alignment="CENTER" style="&#10;" text="Add Project" textAlignment="CENTER" wrapText="false">
                     <textFill>
                        <Color blue="0.624" green="0.624" red="0.624" fx:id="x22111" />
                     </textFill>
                     <font>
                        <Font size="14.0" />
                     </font>
                  </Label>
                  <HBox>
                     <children>
                        <VBox>
                           <children>
                              <Label fx:id="labelProjectName" alignment="CENTER" style="&#10;" text="Projectname" textAlignment="CENTER" textFill="#383838" wrapText="false" />
                              <TextField fx:id="textFieldProjectName" prefHeight="20.0" prefWidth="150.0" />
                           </children>
                           <HBox.margin>
                              <Insets />
                           </HBox.margin>
                           <padding>
                              <Insets right="20.0" />
                           </padding>
                        </VBox>
                        <VBox layoutX="10.0" layoutY="20.0">
                           <children>
                              <Label fx:id="labelProjectManager" alignment="CENTER" style="&#10;" text="Projectmanager" textAlignment="CENTER" textFill="#383838" wrapText="false" />
                              <ChoiceBox fx:id="choiceBoxProjectManager" prefHeight="20.0" prefWidth="150.0" />
                           </children>
                           <padding>
                              <Insets left="20.0" />
                           </padding>
                        </VBox>
                     </children>
                     <VBox.margin>
                        <Insets />
                     </VBox.margin>
                     <padding>
                        <Insets bottom="10.0" top="10.0" />
                     </padding>
                  </HBox>
                  <HBox>
                     <children>
                        <VBox fillWidth="false">
                           <children>
                              <Label fx:id="labelDescription" alignment="CENTER" style="&#10;" text="Description" textAlignment="CENTER" textFill="#383838" wrapText="false" />
                              <TextArea fx:id="textAreaDescription" prefHeight="100.0" prefWidth="340.0" />
                           </children>
                           <padding>
                              <Insets bottom="10.0" top="10.0" />
                           </padding>
                        </VBox>
                     </children>
                     <padding>
                        <Insets bottom="10.0" top="10.0" />
                     </padding>
                  </HBox>
                  <HBox layoutX="20.0" layoutY="102.0">
                     <padding>
                        <Insets bottom="10.0" top="50.0" />
                     </padding>
                     <children>
                        <Button fx:id="buttonSubmit" mnemonicParsing="false" prefHeight="30.0" prefWidth="190.0" text="Submit" />
                        <Button fx:id="buttonCancel" layoutX="10.0" layoutY="20.0" mnemonicParsing="false" prefHeight="30.0" prefWidth="190.0" text="Cancel" />
                     </children>
                  </HBox>
               </children>
               <padding>
                  <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
               </padding>
            </VBox>
         </children>
      </AnchorPane>
